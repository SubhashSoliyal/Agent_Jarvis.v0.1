# LiveKit Configuration (Real-time Audio/Video)
LIVEKIT_URL=wss://your-project.livekit.cloud
LIVEKIT_API_KEY=your_api_key
LIVEKIT_API_SECRET=your_api_secret

# Google Gemini API
GOOGLE_API_KEY=your_gemini_api_key

# Google Search API (for Online Capabilities)
GOOGLE_SEARCH_API_KEY=your_google_search_key
SEARCH_ENGINE_ID=your_search_engine_id

# Weather API (OpenWeatherMap)
OPENWEATHER_API_KEY=your_openweather_key

# DeepSeek / Browsing Automation
GOOGLE_EMAIL=your_email_for_automation@gmail.com
GOOGLE_PASSWORD=your_password
# Note: For DeepSeek, these are used to auto-login if needed.

# User Personalization
USER_NAME=Subhash Chandra
USER_COLLEGE_NAME=Your College
USER_ADDRESS=Your Address

# Memory System (Mem0)
MEM0_API_KEY=your_mem0_key

# Optional: Avatar & Video
ENABLE_VIDEO=true
